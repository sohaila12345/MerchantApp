<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbar"/>
    <ImageView
            android:layout_width="match_parent"
            android:layout_height="290dp"
            app:srcCompat="@drawable/hd"
            android:id="@+id/imageView"/>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:layout_marginTop="70dp" android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp">

        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="110dp" android:layout_marginLeft="8dp"
                android:background="@drawable/rounded_search"
                android:layout_marginRight="8dp"
                android:layout_marginTop="460dp">
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_marginTop="70dp">
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_remove_24"
                        android:id="@+id/imageView20" android:layout_marginTop="8dp"
                        android:layout_marginLeft="220dp" tools:ignore="VectorDrawableCompat"/>
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_add_24"
                        android:id="@+id/imageView16" android:layout_marginLeft="330dp"
                        android:layout_marginTop="7dp" tools:ignore="VectorDrawableCompat"/>
            </RelativeLayout>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/cv" android:id="@+id/imageView2"
                    android:layout_marginLeft="320dp" android:layout_marginTop="15dp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="110dp" android:layout_marginLeft="8dp"
                android:background="@drawable/rounded_search"
                android:layout_marginRight="8dp"
                android:layout_marginTop="70dp">
            <ImageView
                    android:layout_width="134dp"
                    android:layout_height="79dp" app:srcCompat="@drawable/pp" android:id="@+id/imageView21"/>
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_marginTop="70dp">

                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_add_24"
                        android:id="@+id/imageView13" android:layout_marginLeft="330dp"
                        android:layout_marginTop="7dp" tools:ignore="VectorDrawableCompat"/>
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_remove_24"
                        android:id="@+id/imageView17" android:layout_marginTop="8dp"
                        android:layout_marginLeft="220dp" tools:ignore="VectorDrawableCompat"/>
            </RelativeLayout>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="25dp" app:srcCompat="@drawable/cv" android:id="@+id/imageView5"
                    android:layout_marginLeft="320dp" android:layout_marginTop="15dp"/>
            <TextView
                    android:text="Fiber Pillow"
                    android:layout_width="150dp"
                    android:layout_height="30dp" android:id="@+id/textView3" android:layout_marginLeft="130dp"
                    android:layout_marginTop="10dp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="110dp" android:layout_marginLeft="8dp"
                android:background="@drawable/rounded_search"
                android:layout_marginRight="8dp"
                android:layout_marginTop="200dp">
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_marginTop="70dp">
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_remove_24"
                        android:id="@+id/imageView18" android:layout_marginTop="8dp"
                        android:layout_marginLeft="220dp" tools:ignore="VectorDrawableCompat"/>
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_add_24"
                        android:id="@+id/imageView14" android:layout_marginLeft="330dp"
                        android:layout_marginTop="7dp" tools:ignore="VectorDrawableCompat"/>
            </RelativeLayout>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="25dp" app:srcCompat="@drawable/cv" android:id="@+id/imageView4"
                    android:layout_marginLeft="320dp" android:layout_marginTop="15dp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="110dp" android:layout_marginLeft="8dp"
                android:background="@drawable/rounded_search"
                android:layout_marginRight="8dp"
                android:layout_marginTop="330dp">
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:layout_marginTop="70dp">
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_remove_24"
                        android:id="@+id/imageView19" android:layout_marginTop="8dp"
                        android:layout_marginLeft="220dp" tools:ignore="VectorDrawableCompat"/>
                <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        app:srcCompat="@drawable/ic_baseline_add_24"
                        android:id="@+id/imageView15" android:layout_marginLeft="330dp"
                        android:layout_marginTop="7dp" tools:ignore="VectorDrawableCompat"/>
            </RelativeLayout>
            <ImageView
                    android:layout_width="40dp"
                    android:layout_height="25dp" app:srcCompat="@drawable/cv" android:id="@+id/imageView3"
                    android:layout_marginLeft="320dp" android:layout_marginTop="15dp"/>
        </androidx.cardview.widget.CardView>
        <SearchView
                android:layout_width="match_parent"
                android:background="@drawable/rounded_search"
                android:iconifiedByDefault="false"
                android:queryHint="Enter Stock Name"
                android:layout_height="50dp" android:layout_marginTop="10dp" android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"/>
    </RelativeLayout>
</RelativeLayout>